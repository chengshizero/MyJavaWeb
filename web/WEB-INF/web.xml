<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>MySS</servlet-name>
        <servlet-class>pages.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySS</servlet-name>
        <url-pattern>/PP/MyMy.php</url-pattern>
        <url-pattern>/PP/MyMy.cgi</url-pattern>
        <url-pattern>/QQ/MyS.asp</url-pattern>        
        <url-pattern>/QQ/MyS.aspx</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
